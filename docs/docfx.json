{
  "globalMetadata": {
    "_appTitle": ".NEXT",
    "_appLogoPath": "logo.png",
    "_appFaviconPath": "favicon.ico"
  },
  "metadata": [
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "src": "../src/DotNext/"
        },
        {
          "files": [ "**.csproj" ],
          "src": "../src/DotNext.Reflection/"
        },
        {
          "files": [ "**.csproj" ],
          "src": "../src/DotNext.Metaprogramming/"
        },
        {
          "files": [ "**.csproj" ],
          "src": "../src/DotNext.Unsafe/"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}