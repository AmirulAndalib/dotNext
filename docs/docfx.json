{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "src": "../src/DotNext/"
        },
        {
          "files": [ "**.csproj" ],
          "src": "../src/DotNext.IO/"
        },
        {
          "files": [ "**.csproj" ],
          "src": "../src/DotNext.Reflection/"
        },
        {
          "files": [ "**.csproj" ],
          "src": "../src/DotNext.Metaprogramming/"
        },
        {
          "files": [ "**.csproj" ],
          "src": "../src/DotNext.Unsafe/"
        },
        {
          "files": [ "**.csproj" ],
          "src": "../src/DotNext.Threading/"
        },
        {
          "files": [ "**.csproj" ],
          "src": "../src/cluster/DotNext.Net.Cluster"
        }
      ],
      "properties": {
        "TargetFramework": "netstandard2.1"
      },
      "force": true,
      "dest": "api",
    },
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "src": "../src/cluster/DotNext.AspNetCore.Cluster/"
        }
      ],
      "properties": {
        "TargetFramework": "netcoreapp3.1"
      },
      "force": true,
      "dest": "api"
    }
  ],
  "build": {
    "xrefService": ["https://xref.docs.microsoft.com/query?uid={uid}"],
    "globalMetadata": {
      "_appTitle": ".NEXT",
      "_appLogoPath": "doc_logo.png",
      "_appFaviconPath": "fav.ico"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "features/**.md",
          "features/**/toc.yml",
          "toc.yml",
          "*.md",
          "migration/**.md",
          "migration/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "templates/darkfx/darkfx"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}