{
  "build": {
    "xrefService": ["https://xref.docs.microsoft.com/query?uid={uid}"],
    "xref": ["DotNext.xref", "DotNext.IO.xref", "DotNext.Metaprogramming.xref", "DotNext.Reflection.xref", "DotNext.Threading.xref", "DotNext.Unsafe.xref", "DotNext.Net.Cluster.xref", "DotNext.AspNetCore.Cluster.xref"],
    "globalMetadata": {
      "_appTitle": ".NEXT",
      "_appLogoPath": "doc_logo.png",
      "_appFaviconPath": "fav.ico"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "features/**.md",
          "features/**/toc.yml",
          "toc.yml",
          "*.md",
          "migration/**.md",
          "migration/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "templates/darkfx/darkfx"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}